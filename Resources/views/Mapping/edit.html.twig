{% extends "AstinaRedirectManagerBundle::content_layout.html.twig" %}
{% trans_default_domain "AstinaRedirectManagerBundle" %}

{% block armb_content  %}
    <form action="{{ path('armb_update_map', {id:map.id}) }}" method="post" {{ form_enctype(form) }} novalidate>
        {{ form_widget(form) }}
        <div class="form-actions">
            <input class="btn btn-primary" type="submit" value= "{{ "form.save" | trans }}" />
            <a href="{{ path('armb_homepage') }}" class="btn">{{ 'form.abort' | trans }}</a>
        </div>
    </form>
{% endblock %}
